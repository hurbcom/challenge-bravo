{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                               
    "outDir": "./dist",                                   
    "esModuleInterop": true,                        
    "forceConsistentCasingInFileNames": true,      
    "strictPropertyInitialization": false,      
    "strict": true,                                      
    "skipLibCheck": true,         
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noEmitOnError": true                
  }
}
