{
  "compilerOptions": {
  "target": "es5",                   
  "module": "commonjs",
  "rootDir": "./",
  "baseUrl": "./src",
  "paths": {
    "@modules/*": ["modules/*"],
    "@config/*": ["config/*"],
    "@shared/*": ["shared/*"],
    "@errors/*": ["errors/*"],
    "@utils/*": ["utils/*"]
  },
  "esModuleInterop": true,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true
  }
}