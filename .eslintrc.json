{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true

    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "rules": {
        "no-useless-constructor":"off"
    }
}