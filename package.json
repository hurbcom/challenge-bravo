{
  "name": "challenge-bravo",
  "version": "1.0.0",
  "repository": "git@github.com:YDoro/challenge-bravo.git",
  "author": "Yan Mello <yan_mvd@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node  dist/main/server.js",
    "dev": "npm run build && docker-compose up -d",
    "watch": "nodemon src/main/server.ts",
    "build": "tsc",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "yarn test --watch -c jest-unit-config.js",
    "test:integration": "yarn test  --watch -c jest-integration-config.js",
    "test:staged": "yarn test --findRelatedTests --forceExit",
    "test:ci": "yarn test --coverage --forceExit"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.2.1",
    "node-schedule": "^2.1.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/node-schedule": "^1.3.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.2.5",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0"
  }
}
